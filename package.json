{
  "name": "tape-six-proc",
  "version": "1.2.3",
  "description": "Process-isolated test runner for tape-six. Runs each test file in its own subprocess. Works with Node, Deno, and Bun. Supports TypeScript without transpilation.",
  "type": "module",
  "bin": {
    "tape6-proc": "bin/tape6-proc.js"
  },
  "scripts": {
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "node ./bin/tape6-proc.js --flags FO",
    "test:bun": "bun run ./bin/tape6-proc.js --flags FO",
    "test:deno": "deno run -A ./bin/tape6-proc.js -r --allow-read -r --allow-env --flags FO"
  },
  "keywords": [
    "tap",
    "tape6",
    "tape-six",
    "test",
    "test-runner",
    "unit-test",
    "testing",
    "harness",
    "process",
    "subprocess",
    "isolation",
    "parallel",
    "cross-runtime",
    "nodejs",
    "deno",
    "bun",
    "esm",
    "es-modules",
    "typescript",
    "no-transpile"
  ],
  "author": "Eugene Lazutkin <eugene.lazutkin@gmail.com> (https://www.lazutkin.com)",
  "funding": "https://github.com/sponsors/uhop",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uhop/tape-six-proc.git"
  },
  "bugs": {
    "url": "https://github.com/uhop/tape-six-proc/issues"
  },
  "homepage": "https://github.com/uhop/tape-six-proc#readme",
  "github": "http://github.com/uhop/tape-six-proc",
  "llms": "https://raw.githubusercontent.com/uhop/tape-six-proc/master/llms.txt",
  "llmsFull": "https://raw.githubusercontent.com/uhop/tape-six-proc/master/llms-full.txt",
  "files": [
    "bin",
    "src",
    "llms.txt",
    "llms-full.txt"
  ],
  "dependencies": {
    "dollar-shell": "^1.1.10",
    "tape-six": "^1.7.4"
  },
  "tape6": {
    "tests": [
      "/tests/test-*.*js"
    ]
  },
  "devDependencies": {
    "chai": "^6.2.2"
  }
}
